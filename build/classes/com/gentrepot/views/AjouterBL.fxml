        </columns>
      </TableView>
      <Button fx:id="btnLivrer" layoutX="367.0" layoutY="630.0" mnemonicParsing="false" onAction="#afficherInterfaceLivreer" text="Livreer" />
      <Label layoutX="450.0" layoutY="14.0" prefHeight="45.0" prefWidth="202.0" text="Liste des commandes">
