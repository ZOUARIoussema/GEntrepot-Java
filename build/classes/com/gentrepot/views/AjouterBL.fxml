        <URL value="@ajouterbl.css" />
    </stylesheets>
   <children>
      <TableView fx:id="tableViewCommande" layoutX="46.0" layoutY="67.0" onMouseClicked="#selectCommande" prefHeight="540.0" prefWidth="1010.0">
