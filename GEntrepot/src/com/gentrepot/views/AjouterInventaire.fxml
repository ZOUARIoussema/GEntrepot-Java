<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="522.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gentrepot.controllers.AjouterInventaireController">
    <stylesheets>
        <URL value="@ajouterinventaire.css" />
    </stylesheets>
   <children>
      <GridPane alignment="CENTER" layoutX="90.0" layoutY="55.0" prefHeight="244.0" prefWidth="421.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="103.0" minWidth="10.0" prefWidth="91.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="88.0" minWidth="10.0" prefWidth="34.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="162.0" minWidth="10.0" prefWidth="96.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="155.0" minWidth="10.0" prefWidth="155.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="46.0" minHeight="10.0" prefHeight="46.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="29.0" minHeight="10.0" prefHeight="22.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <Label alignment="CENTER_RIGHT" text="Solde total">
               <GridPane.margin>
                  <Insets top="25.0" />
               </GridPane.margin>
            </Label>
            <Label fx:id="soldeTC" text="Solde en cheque" GridPane.rowIndex="1">
               <GridPane.margin>
                  <Insets top="25.0" />
               </GridPane.margin>
            </Label>
            <Label fx:id="soldeTE" text="Solde en espece" GridPane.rowIndex="2">
               <GridPane.margin>
                  <Insets top="25.0" />
               </GridPane.margin>
            </Label>
            <Label fx:id="soldeCalculer" text="Solde calculer" GridPane.rowIndex="3">
               <padding>
                  <Insets top="25.0" />
               </padding>
            </Label>
            <Label fx:id="ecart" text="Ecart" GridPane.rowIndex="4">
               <padding>
                  <Insets top="25.0" />
               </padding>
            </Label>
            <JFXTextField fx:id="totalTheorique" editable="false" GridPane.columnIndex="2" />
            <JFXTextField fx:id="totalCheque" editable="false" GridPane.columnIndex="2" GridPane.rowIndex="1" />
            <JFXTextField fx:id="totalEspece" editable="false" GridPane.columnIndex="2" GridPane.rowIndex="2" />
            <JFXTextField fx:id="soldeTCalculer" onKeyReleased="#calculerEcart" GridPane.columnIndex="2" GridPane.rowIndex="3" />
            <JFXTextField fx:id="ecartC" GridPane.columnIndex="2" GridPane.rowIndex="4" />
         </children>
      </GridPane>
      <JFXButton fx:id="btntAjouter" layoutX="290.0" layoutY="299.0" onAction="#ajouterInventaire" text="Valider" />
      <JFXButton fx:id="btnAnnuler" layoutX="359.0" layoutY="299.0" onAction="#anuuler" text="Annuler" />
   </children>
</AnchorPane>
